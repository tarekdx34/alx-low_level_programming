#include <stdio.h>
#include <stdlib.h>
#include "dog.h"
/**
 * print_dog -  prints a struct dog
 * @d :dog structure pointer
 *
 * Return: Nothing
 */
void print_dog(struct dog *d)
{
if (d != NULL && d->name != NULL)
{
printf("Name: %s", d->name);
printf("Age: %f", d->age);
printf("Owner: %s", d->owner);

}
if (d->name == NULL || d->owner == NULL)
printf("(nil)");
}
